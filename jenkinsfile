pipeline{
    agent any
    stages {
        stage ('checkout') {
            steps {
                sh "java -version"
            }
        }
    stage ('build') {
            steps {
                sh "java -version"
            }
        }
    stage ('artifact') {
            steps {
                sh "java -version"
            }
        }
    }
post {
    always {
        echo "pipeline super ra Bala"
    }    
    }    
    
}
